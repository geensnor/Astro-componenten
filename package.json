{
  "name": "@geensnor/astro-componenten",
  "version": "2.0.2",
  "description": "Paar prachtige Astro componenten in Geensnor stijl",
  "homepage": "https://github.com/geensnor/astro-componenten#readme",
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./de-snor": "./src/de-snor.astro",
    "./kop": "./src/kop.astro",
    "./knoepert": "./src/knoepert.astro",
    "./lijstitem": "./src/lijstitem.astro",
    "./tegel": "./src/tegel.astro"
  },
  "files": [
    "index.ts",
    "src/*",
    "assets/*",
    "fonts/*"
  ],
  "keywords": [
    "astro-component",
    "geensnor"
  ],
  "scripts": {
    "format": "npx prettier -w src/*",
    "lint": "eslint --ext .ts,.js,.astro src/*"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "eslint-plugin-astro": "^0.29.1",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0",
    "typescript": "^5.2.2"
  }
}
